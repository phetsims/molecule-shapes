{
  "name": "molecule-shapes",
  "version": "1.7.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/molecule-shapes.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "MOLECULE_SHAPES",
    "phetLibs": [
      "mobius",
      "nitroglycerin"
    ],
    "preload": [
      "../sherpa/lib/typedarray-44f09cf.js",
      "../sherpa/lib/three-r71.js",
      "../sherpa/lib/three-r71-Projector.js",
      "../sherpa/lib/three-r71-CanvasRenderer.js",
      "../sherpa/lib/liberation-sans-regular-subset-numeric-degrees.js"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "simulation": true,
    "supportsOutputJS": true,
    "simFeatures": {
      "colorProfiles": [
        "default",
        "projector"
      ]
    },
    "published": true,
    "phet-io": {
      "compareDesignedAPIChanges": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "globals": {
      "THREE": "readonly"
    },
    "rules": {
      "todo-should-have-issue": "off",
      "no-view-imported-from-model": "off"
    }
  }
}